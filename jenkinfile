pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
            stage('build') {
            steps {
                echo 'building'
            }
            }
            stage('test') {
            steps {
                echo 'testing'
            }
            }
            stage('deploy') {
            steps {
                echo 'deploying'
            }
            }
            stage('release') {
            steps {
                echo 'releasing'
            }
            
        }
    }
}
